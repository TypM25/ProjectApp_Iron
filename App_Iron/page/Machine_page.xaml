<Window x:Class="App_Iron.Machine_page"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:App_Iron"
        mc:Ignorable="d"
        Title="Machine_page" Height="450" Width="800">
    <Grid>
        <Label Content="Product" HorizontalAlignment="Left" Margin="41,27,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" FontFamily="Bahnschrift"/>
        <Button x:Name="btnCreateproduct" Content="Create" HorizontalAlignment="Left" Margin="524,58,0,0" VerticalAlignment="Top" Height="49" Width="171" Click="btnCreateproduct_Click" FontSize="18" FontFamily="Bahnschrift" BorderBrush="{x:Null}"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="524,138,0,0" VerticalAlignment="Top" Height="49" Width="171" FontSize="18" FontFamily="Bahnschrift" BorderBrush="{x:Null}"/>
        <TextBox HorizontalAlignment="Left" Margin="64,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="336" Height="313" Background="#FFD4D0D0"/>

    </Grid>
</Window>
